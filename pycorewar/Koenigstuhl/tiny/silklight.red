;redcode-94x
;name Silk Light
;author Christian Schmidt
;strategy Paper for the tiny hill
;assert 1

pStep1 equ 64
pStep2 equ 79

start  spl 1
       spl 1

       mov   {pap1,  {pBoo
pBoo:  spl   pEnd+600,{493

       mov   {pEnd,  {pBoo2
pBoo2: jmp   pEnd+200,{493

for 9
dat 0, 0
rof

pap1:  spl   @pEnd,  <pStep1
       mov.i }pap1,  >pap1
       mov.i {pap1,  <pap2
pap2:  jmp.a @0,     pStep2
pEnd:  dat   0,      0

end start

